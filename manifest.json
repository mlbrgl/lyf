{
  "name": "Lyf",
  "description": "Keep reading. Links can wait.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Toggle",
    "default_icon": "icons/icon16_inactive.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
